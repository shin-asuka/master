<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
	    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd
	    http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.2.xsd">

	<bean id="neoClient" class="com.vipkid.neo.client.NeoClient">
		<constructor-arg name="host" value="${microservice.payroll.host}" />
		<constructor-arg name="port" value="${microservice.payroll.port}" />
		<constructor-arg name="timeoutSeconds"
			value="${microservice.payroll.timeout.seconds}" />
	</bean>

	<bean id="jobNode" class="com.vipkid.vschedule.client.JobNode"
		init-method="init">
		<property name="doJobServicePort" value="${rpc.job.port}" />
		<property name="appName" value="TP" />
	</bean>


</beans>
