<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TeacherQuizMapper">

	<sql id="TeacherQuizColumns">
		teacher_quiz.id AS teacher_quiz_id, 
		teacher_quiz.quiz_score AS teacher_quiz_quiz_score, 
		teacher_quiz.creation_time AS teacher_quiz_creation_time,
		teacher_quiz.start_quiz_time AS teacher_quiz_start_quiz_time,
		teacher_quiz.quiz_time AS teacher_quiz_quiz_time,
		teacher_quiz.teacher_id AS teacher_quiz_teacher_id,
		teacher_quiz.update_time AS teacher_quiz_update_time,
		teacher_quiz.update_id	AS teacher_quiz_update_id, 
		teacher_quiz.status AS teacher_quiz_status
	</sql>

	<sql id="TeacherQuizWhere">
		<trim prefix="WHERE" prefixOverrides="AND">
			<if test="id!=0">AND teacher_quiz.id = #{id}</if>
			<if test="quizScore!=0">AND teacher_quiz.quiz_score = #{quizScore}</if>
			<if test="creationTime!=null">AND teacher_quiz.creation_time = #{creationTime}</if>
			<if test="startQuizTime!=null">AND teacher_quiz.start_quiz_time = #{startQuizTime}</if>
			<if test="quizTime!=null">AND teacher_quiz.quiz_time = #{quizTime}</if>
			<if test="teacherId!=0">AND teacher_quiz.teacher_id = #{teacherId}</if>
			<if test="updateTime!=null">AND teacher_quiz.update_time = #{updateTime}</if>
			<if test="updateId !=0">AND teacher_quiz.update_id = #{updateId}</if>
			<if test="status != -1">AND teacher_quiz.status = #{status}</if>
			<if test="andwhere!=null"> ${andwhere} </if>
		</trim>
	</sql>

	<sql id="TeacherQuizOrder">
		<if test="orderString!=null">ORDER BY ${orderString}</if>
	</sql>

	<sql id="TeacherQuizLimit">LIMIT #{startLine},#{limitLine}</sql>

	<resultMap type="com.vipkid.trpm.entity.TeacherQuiz" id="TeacherQuizResultMap">
		<id column="teacher_quiz_id" property="id" />
		<result column="teacher_quiz_quiz_score" property="quizScore" />
		<result column="teacher_quiz_creation_time" property="creationTime" />
		<result column="teacher_quiz_start_quiz_time" property="startQuizTime" />
		<result column="teacher_quiz_quiz_time" property="quizTime" />
		<result column="teacher_quiz_teacher_id" property="teacherId" />
		<result column="teacher_quiz_update_time" property="updateTime" />
		<result column="teacher_quiz_update_id" property="updateId" />
		<result column="teacher_quiz_status" property="status" />
	</resultMap>

	<select id="findDao" resultMap="TeacherQuizResultMap">
		SELECT
		<include refid="TeacherQuizColumns" />
		FROM teacher_quiz
		<include refid="TeacherQuizWhere" />
		<include refid="TeacherQuizOrder" />
	</select>

	<select id="countDao" resultType="int">
		SELECT COUNT(*) FROM teacher_quiz
		<include refid="TeacherQuizWhere" />
	</select>

	<select id="pageDao" resultMap="TeacherQuizResultMap">
		SELECT
		<include refid="TeacherQuizColumns" />
		FROM teacher_quiz
		<include refid="TeacherQuizWhere" />
		<include refid="TeacherQuizOrder" />
		<include refid="TeacherQuizLimit" />
	</select>

	<select id="limitDao" resultMap="TeacherQuizResultMap">
		SELECT
		<include refid="TeacherQuizColumns" />
		FROM teacher_quiz
		<include refid="TeacherQuizWhere" />
		<include refid="TeacherQuizOrder" />
		<include refid="TeacherQuizLimit" />
	</select>

	<insert id="saveDao" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO teacher_quiz
		<trim prefix="(" prefixOverrides=",">
			<if test="id!=0">,id</if>
			<if test="quizScore!=0">,quiz_score</if>
			<if test="creationTime!=null">,creation_time</if>
			<if test="startQuizTime!=null">,start_quiz_time</if>
			<if test="quizTime!=0">,quiz_time</if>
			<if test="updateTime!=null">,update_time</if>
			<if test="teacherId!=0">,teacher_id</if>
			<if test="updateId!=0">,update_id</if>
			<if test="status!=0">,status</if>
		</trim>
		) VALUES
		<trim prefix="(" prefixOverrides=",">
			<if test="id!=0">,#{id}</if>
			<if test="quizScore!=0">,#{quizScore}</if>
			<if test="creationTime!=null">,#{creationTime}</if>
			<if test="startQuizTime!=null">,#{startQuizTime}</if>
			<if test="quizTime!=0">,#{quizTime}</if>
			<if test="updateTime!=null">,#{updateTime}</if>
			<if test="teacherId!=0">,#{teacherId}</if>
			<if test="updateId!=0">,#{updateId}</if>
			<if test="status!=0">,#{status}</if>
		</trim>
		)
	</insert>

	<update id="updateDao">
		UPDATE teacher_quiz
		<trim prefix="SET" prefixOverrides=",">
			<if test="id!=0">,id = #{id}</if>
			<if test="quizScore!=0">,quiz_score = #{quizScore}</if>
			<if test="creationTime!=null">,creation_time = #{creationTime}</if>
			<if test="startQuizTime!=null">,start_quiz_time = #{startQuizTime}</if>
			<if test="quizTime!=null">,quiz_time = #{quizTime}</if>
			<if test="updateTime!=null">,update_time = #{updateTime}</if>
			<if test="teacherId!=0">,teacher_id = #{teacherId}</if>
			<if test="updateId!=0">,update_id = #{updateId}</if>
			<if test="status!=0">,status = #{status}</if>
		</trim>
		<trim prefix="WHERE" prefixOverrides="AND">AND id = #{id}</trim>
	</update>

	<delete id="deleteDao">
		DELETE FROM teacher_quiz
		<trim prefix="WHERE" prefixOverrides="AND">AND id = #{id}</trim>
	</delete>

</mapper>
