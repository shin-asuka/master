<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TeacherQuizDetailsMapper">

	<sql id="teacherQuizDetailsColumns">teacher_quiz_details.id AS id,
		teacher_quiz_details.teacher_id AS teacher_id, teacher_quiz_details.sn
		AS sn, teacher_quiz_details.teacher_answer AS teacher_answer,
		teacher_quiz_details.correct_answer AS correct_answer,
		teacher_quiz_details.quiz_id AS quiz_id, teacher_quiz_details.score AS
		score</sql>

	<sql id="teacherQuizDetailsWhere">
		<trim prefix="WHERE" prefixOverrides="AND">
			<if test="id != 0">AND teacher_quiz_details.id = #{id}</if>
			<if test="teacherId != 0">AND teacher_quiz_details.teacher_id = #{teacherId}</if>
			<if test="sn != null">AND teacher_quiz_details.sn = #{sn}</if>
			<if test="teacherAnswer != 0">AND teacher_quiz_details.teacher_answer =
				#{teacherAnswer}</if>
			<if test="correctAnswer != 0">AND teacher_quiz_details.correct_answer =
				#{correctAnswer}</if>
			<if test="quizId != 0">AND teacher_quiz_details.quiz_id = #{quizId}</if>
			<if test="score != 0">AND teacher_quiz_details.score = #{score}</if>
		</trim>
	</sql>

	<sql id="teacherQuizDetailsOrder">
		<if test="orderString!=null">ORDER BY ${orderString}</if>
	</sql>

	<sql id="teacherQuizDetailsLimit">LIMIT #{startLine}, #{limitLine}</sql>

	<resultMap type="com.vipkid.trpm.entity.TeacherQuizDetails"
		id="teacherQuizDetailsResultMap">
		<id column="id" property="id" />
		<result column="teacher_id" property="teacherId" />
		<result column="sn" property="sn" />
		<result column="teacher_answer" property="teacherAnswer" />
		<result column="correct_answer" property="correctAnswer" />
		<result column="quiz_id" property="quizId" />
		<result column="score" property="score" />
	</resultMap>

	<select id="findDao" resultMap="teacherQuizDetailsResultMap">
		SELECT
		<include refid="teacherQuizDetailsColumns" />
		FROM teacher_quiz_details
		<include refid="teacherQuizDetailsWhere" />
	</select>

	<select id="countDao" resultType="int">
		SELECT COUNT(*) FROM teacher_quiz_details
		<include refid="teacherQuizDetailsWhere" />
	</select>

	<select id="pageDao" resultMap="teacherQuizDetailsResultMap">
		SELECT
		<include refid="teacherQuizDetailsColumns" />
		FROM teacher_quiz_details
		<include refid="teacherQuizDetailsWhere" />
		<include refid="teacherQuizDetailsOrder" />
		<include refid="teacherQuizDetailsLimit" />
	</select>

	<select id="limitDao" resultMap="teacherQuizDetailsResultMap">
		SELECT
		<include refid="teacherQuizDetailsColumns" />
		FROM teacher_quiz_details
		<include refid="teacherQuizDetailsWhere" />
		<include refid="teacherQuizDetailsOrder" />
		<include refid="teacherQuizDetailsLimit" />
	</select>

	<insert id="saveDao" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO teacher_quiz_details
		<trim prefix="(" prefixOverrides=",">
			<if test="id != 0">,id</if>
			<if test="teacherId != 0">,teacher_id</if>
			<if test="sn != null">,sn</if>
			<if test="teacherAnswer != 0">,teacher_answer</if>
			<if test="correctAnswer != 0">,correct_answer</if>
			<if test="quizId != 0">,quiz_id</if>
			<if test="score != 0">,score</if>
		</trim>
		) VALUES
		<trim prefix="(" prefixOverrides=",">
			<if test="id != 0">,#{id}</if>
			<if test="teacherId != 0">,#{teacherId}</if>
			<if test="sn != null">,#{sn}</if>
			<if test="teacherAnswer != 0">,#{teacherAnswer}</if>
			<if test="correctAnswer != 0">,#{correctAnswer}</if>
			<if test="quizId != 0">,#{quizId}</if>
			<if test="score != 0">,#{score}</if>
		</trim>
		)
	</insert>

	<update id="updateDao">
		UPDATE teacher_quiz_details
		<trim prefix="SET" prefixOverrides=",">
			<if test="id != 0">,id = #{id}</if>
			<if test="teacherId != 0">,teacher_id = #{teacherId}</if>
			<if test="sn != null">,sn = #{sn}</if>
			<if test="teacherAnswer != 0">,teacher_answer = #{teacherAnswer}</if>
			<if test="correctAnswer != 0">,correct_answer = #{correctAnswer}</if>
			<if test="quizId != 0">,quiz_id = #{quizId}</if>
			<if test="score != 0">,score = #{score}</if>
		</trim>
		<trim prefix="WHERE" prefixOverrides="AND">AND id = #{id}</trim>
	</update>

	<delete id="deleteDao">
		DELETE FROM teacher_quiz_details
		<trim prefix="WHERE" prefixOverrides="AND">AND id = #{id}</trim>
	</delete>

</mapper>
