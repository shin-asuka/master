<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TeacherPageLoginMapper">

	<sql id="teacherPageLoginColumns">teacher_page_login.id AS teacher_page_login_id, teacher_page_login.user_id AS
		teacher_page_login_user_id, teacher_page_login.login_type AS teacher_page_login_login_type</sql>

	<sql id="teacherPageLoginWhere">
		<trim prefix="WHERE" prefixOverrides="AND">
			<if test="id!=0">AND teacher_page_login.id = #{id}</if>
			<if test="userId!=0">AND teacher_page_login.user_id = #{userId}</if>
			<if test="loginType!=-1">AND teacher_page_login.login_type = #{loginType}</if>
		</trim>
	</sql>

	<sql id="teacherPageLoginOrder">
		<if test="orderString!=null">ORDER BY ${orderString}</if>
	</sql>

	<sql id="teacherPageLoginLimit">LIMIT #{startLine},#{limitLine}</sql>

	<resultMap type="com.vipkid.trpm.entity.TeacherPageLogin" id="teacherPageLoginResultMap">
		<id column="teacher_page_login_id" property="id" />
		<result column="teacher_page_login_user_id" property="userId" />
		<result column="teacher_page_login_login_type" property="loginType" />
	</resultMap>

	<select id="findDao" resultMap="teacherPageLoginResultMap">
		SELECT
		<include refid="teacherPageLoginColumns" />
		FROM teacher_page_login
		<include refid="teacherPageLoginWhere" />
		<include refid="teacherPageLoginOrder" />
	</select>

	<select id="countDao" resultType="int">
		SELECT COUNT(*) FROM teacher_page_login
		<include refid="teacherPageLoginWhere" />
	</select>

	<select id="pageDao" resultMap="teacherPageLoginResultMap">
		SELECT
		<include refid="teacherPageLoginColumns" />
		FROM teacher_page_login
		<include refid="teacherPageLoginWhere" />
		<include refid="teacherPageLoginOrder" />
		<include refid="teacherPageLoginLimit" />
	</select>

	<select id="limitDao" resultMap="teacherPageLoginResultMap">
		SELECT
		<include refid="teacherPageLoginColumns" />
		FROM teacher_page_login
		<include refid="teacherPageLoginWhere" />
		<include refid="teacherPageLoginOrder" />
		<include refid="teacherPageLoginLimit" />
	</select>

	<insert id="saveDao" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO teacher_page_login
		<trim prefix="(" prefixOverrides=",">
			<if test="id!=0">,id</if>
			<if test="userId!=0">,user_id</if>
			<if test="loginType!=-1">,login_type</if>
		</trim>
		) VALUES
		<trim prefix="(" prefixOverrides=",">
			<if test="id!=0">,#{id}</if>
			<if test="userId!=0">,#{userId}</if>
			<if test="loginType!=-1">,#{loginType}</if>
		</trim>
		)
	</insert>

	<update id="updateDao">
		UPDATE teacher_page_login
		<trim prefix="SET" prefixOverrides=",">
			<if test="id!=0">,id = #{id}</if>
			<if test="userId!=0">,user_id = #{userId}</if>
			<if test="loginType!=-1">,login_type = #{loginType}</if>
		</trim>
		<trim prefix="WHERE" prefixOverrides="AND">AND id = #{id}</trim>
	</update>

	<delete id="deleteDao">
		DELETE FROM teacher_page_login
		<trim prefix="WHERE" prefixOverrides="AND">AND id = #{id}</trim>
	</delete>

</mapper>
